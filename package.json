{
  "name": "langchain-agent",
  "version": "1.0.0",
  "description": "A distributed multi-agent system with advanced learning capabilities",
  "main": "src/index.ts",
  "bin": {
    "agent-cli": "dist/scripts/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/server.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "dev:server": "ts-node-dev --respawn src/server.ts",
    "dev:web": "cd web && npm run dev",
    "cli": "ts-node src/scripts/cli.ts",
    "analyze": "ts-node src/scripts/analyze-logs.ts",
    "monitor": "ts-node src/scripts/monitor.ts",
    "train": "ts-node src/scripts/train.ts",
    "manage": "ts-node src/scripts/manage.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"web/src/**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.1.1",
    "@langchain/core": "^0.1.4",
    "@langchain/openai": "^0.0.4",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "langchain": "^0.1.9",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/langchain-agent.git"
  },
  "keywords": [
    "langchain",
    "agents",
    "llm",
    "ai",
    "machine-learning"
  ],
  "author": "Your Name",
  "license": "MIT"
}
